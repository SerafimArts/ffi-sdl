{
    "name": "serafim/ffi-sdl",
    "description": "PHP FFI to SDL2 bindings",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/SerafimArts/ffi-sdl/issues",
        "source": "https://github.com/SerafimArts/ffi-sdl"
    },
    "keywords": [
        "sdl",
        "ffi",
        "bindings",
        "opengl",
        "graphics",
        "library",
        "direct media"
    ],
    "homepage": "https://github.com/SerafimArts/ffi-sdl",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-ffi": "*",
        "ffi/env": "^1.0",
        "ffi/proxy": "^1.0",
        "ffi/location": "^1.0",
        "ffi-headers/contracts": "^1.0",
        "ffi/preprocessor": "^0.2",
        "psr/simple-cache": "^1.0|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\SDL\\": "src"
        }
    },
    "require-dev": {
        "ffi/var-dumper": "^1.0",
        "phpunit/phpunit": "^9.0",
        "symfony/cache": "^5.4|^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\SDL\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev",
            "dev-main": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
